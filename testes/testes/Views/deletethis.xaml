<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Background="Blue"
             x:Class="testes.Views.deletethis">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="not hungry" FontSize ="30" 
               x:Name="labelhunger"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            
            <Label Text="not thirsty" FontSize ="30" 
               x:Name="labelthirsty"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            
            
            <Label Text="not sleepy" FontSize ="30" 
               x:Name="labelsleepy"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            
            
            <Label Text="happy" FontSize ="30" 
               x:Name="labelhappy"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>